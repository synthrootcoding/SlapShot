@model LeagueStandings

@{
    ViewData["Title"] = "LeagueStandings";
}

<h1>@Model.response[0][0].league.name / @Model.response[0][0].league.season</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                1
            </th>
            <th>
                2
            </th>
            <th>
                3
            </th>
            <th>
                4
            </th>
            <th>
                5
            </th>
            <th>
                6
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.response[0]) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.position)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.stage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.points)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.form)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.description)
            </td>
            <td>
            </td>
        </tr>
}
    </tbody>
</table>
